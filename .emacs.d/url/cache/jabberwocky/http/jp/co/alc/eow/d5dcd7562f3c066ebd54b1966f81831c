	<script type="text/javascript" language="javascript">

		//paging_btn_hover
		$(function(){

			//search_word
			var input_val = "検索語を入力してクリック";	//初期のval値
			$('#q').focus(function(){
				var check_val = $(this).val();	//フォーカス時のvalを調べる

				if( check_val == input_val ){
					$(this).val('').css('color', '#333');
				}
			});

			$('#q').blur(function(){
				if( $(this).val() == '' ){
					$('#q').val("検索語を入力してクリック").css('color', '#bebebe');
				}
			});

			//clear btn
			$('#clearBtn').focus(function(){
				$('#q').val('').css('color', '#333');
			});

			//search btn
			$('#search').focus(function(){
				var check_val = $('#q').val();
				if( check_val == input_val ){
					 $('#q').val("").css('color', '#333');;
				}
			});

			//img_hover
			$('.ico_help img').on({
				'mouseover': function(){
					$(this).attr('src', 'http://eow.alc.co.jp/content/img/ico_help_on.png');
				},
				'mouseout': function(){
					$(this).attr('src', 'http://eow.alc.co.jp/content/img/ico_help_off.png');
				}
			});
			$('.ico_setting img').on({
				'mouseover': function(){
					$(this).attr('src', 'http://eow.alc.co.jp/content/img/ico_setting_on.png');
				},
				'mouseout': function(){
					$(this).attr('src', 'http://eow.alc.co.jp/content/img/ico_setting_off.png');
				}
			});


			//fukidashi
			var fukidashi = '<div class="fukidashi"><p class="closeBtn"><img src="http://eow.alc.co.jp/content/img/tango_close.png" alt="閉じる" width="24" height="24" /></p><p class="text">単語帳への登録は「英辞郎 on the WEB Pro」でご利用ください。<br />5000件まで登録できます。</p><p class="btn"><a href="http://eowp.alc.co.jp/info2/?utm_source=serp_pc&utm_medium=popup&utm_campaign=wordbook" target="_blank"><img src="http://eow.alc.co.jp/content/img/tango_btn.png" alt="詳しくはこちら" width="120" height="31" /></a></p></div>';
			$('.tango').append(fukidashi);

			var fukidasi_ex_sentence = '<div class="fukidashi"><p class="closeBtn"><img src="http://eow.alc.co.jp/content/img/tango_close.png" alt="閉じる" width="24" height="24" /></p><p class="text">文例は「英辞郎 on the WEB Pro」（例文総数100万件以上）でご覧ください。</p><p class="btn"><a href="http://eowp.alc.co.jp/info2/?utm_source=eow&utm_medium=popup&utm_campaign=sentence" target="_blank"><img src="http://eow.alc.co.jp/content/img/tango_btn.png" alt="詳しくはこちら" width="120" height="31" /></a></p></div>';
			$('.ex_sentence').append(fukidasi_ex_sentence);

			var tt_flg = false;
			$('.tango_btn').on({
				'click': function(){
					//一旦非表示
					$('.fukidashi').hide();
					$('.tango_btn').attr('src', 'http://eow.alc.co.jp/content/img/tango_off.png');
					//該当表示
					$('.myTT').show();
					$(this).next('.fukidashi').show();
					$(this).attr('src', 'http://eow.alc.co.jp/content/img/tango_on.png');
					tt_flg = true;
				},
				'mouseover': function(){
					if( tt_flg == false ){
						$(this).css('cursor','pointer').attr('src', 'http://eow.alc.co.jp/content/img/tango_on.png');
					}
				},
				'mouseout': function(){
					if( tt_flg == false ){
						$(this).css('cursor','default').attr('src', 'http://eow.alc.co.jp/content/img/tango_off.png');
					}
				}
			});

			$('.closeBtn').on({
				'click': function(){
					fuki_hide();
				},
				'mouseover': function(){
					$(this).css('cursor','pointer');
				},
				'mouseout': function(){
					$(this).css('cursor','default');
				}
			});

			$('.myTT').on({
				'click': function(){
					fuki_hide();
				}
			});

			function fuki_hide(){
				$('.fukidashi').hide();
				$('.myTT').hide();
				$('.tango > img').attr('src', 'http://eow.alc.co.jp/content/img/tango_off.png');
				tt_flg = false;
			}

		});
	</script>
